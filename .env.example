# ==============================
# Project Settings
# ==============================

# Database connection
SQL_CONNECTION_URL=postgresql+asyncpg://fin_user:dev_password@localhost:5432/finance_advisor


# ==============================
# LLM Integration Settings (Ollama)
# ==============================

# Provider: OLLAMA
LLM_INTEGRATION_SETTINGS__PROVIDER=OLLAMA

# Ollama model name (example: llama2, llama3)
LLM_INTEGRATION_SETTINGS__MODEL_NAME=llama2

# Sampling temperature
LLM_INTEGRATION_SETTINGS__TEMPERATURE=0.7

# API key (Ollama local server may not require this)
LLM_INTEGRATION_SETTINGS__API_KEY=your_api_key_here

# Base URL for Ollama server
LLM_INTEGRATION_SETTINGS__BASE_URL=http://localhost:11434

